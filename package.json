{
  "name": "solo-leveling-fitness",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "web",
    "server"
  ],
  "scripts": {
    "start": "npm run db:push && concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "dev:web": "npm run dev -w web",
    "dev:server": "npm run dev -w server",
    "build": "npm run build -w web && npm run build -w server",
    "build:web": "npm run build -w web",
    "build:server": "npm run build -w server",
    "lint": "npm run lint -w web && npm run lint -w server",
    "format": "prettier --write .",
    "db:generate": "npm run db:generate -w server",
    "db:push": "npm run db:push -w server",
    "db:migrate": "npm run db:migrate -w server",
    "db:seed": "npm run db:seed -w server",
    "db:studio": "npm run db:studio -w server",
    "typecheck": "tsc --noEmit -p web && tsc --noEmit -p server"
  },
  "devDependencies": {
    "@browserbasehq/stagehand": "^3.0.7",
    "@eslint/js": "^9.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
